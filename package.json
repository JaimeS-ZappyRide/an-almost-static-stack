{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "highland": "^2.11.1",
    "level": "^2.0.1",
    "prettier": "^1.8.2",
    "react-scripts": "^1.0.17",
    "react-snap": "^1.12.2",
    "readdirp": "^2.1.0",
    "s3-sync-aws": "^1.1.1",
    "sw-precache": "^5.2.0",
    "uglifyjs": "^2.4.11"
  },
  "dependencies": {
    "appcache-nanny": "^1.1.0",
    "loadable-components": "^1.4.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "styled-components": "^2.2.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "generate-sw": "sw-precache --root=build --config scripts/sw-precache-config.js && uglifyjs build/service-worker.js -o build/service-worker.js",
    "generate-appcache": "node scripts/generate-appcache.js",
    "prettier": "prettier --write {*,scripts/*,src/*}.{js,json,css}",
    "postinstall": "yarn run build",
    "postbuild": "react-snap && yarn run generate-sw && yarn run generate-appcache"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "cacheAjaxRequests": true,
    "include": [
      "/shell.html"
    ]
  },
  "engines": {
    "node": "8.6.0",
    "yarn": ">1.3.2"
  }
}
